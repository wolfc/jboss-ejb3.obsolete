<?xml version="1.0" encoding="utf-8"?>
<jboss
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                            http://www.jboss.org/j2ee/schema/jboss_5_0.xsd"
        version="3.0">

   <enterprise-beans>
      <message-driven>
         <ejb-name>ObjectMessageBean</ejb-name>
         <destination-jndi-name>queue/testObjectMessage</destination-jndi-name>
      </message-driven>
      <message-driven>
         <ejb-name>BMTTXTimeOutBean</ejb-name>
         <destination-jndi-name>queue/A</destination-jndi-name>
      </message-driven>
      <message-driven>
         <ejb-name>CMTTXTimeOutBean</ejb-name>
         <destination-jndi-name>queue/B</destination-jndi-name>
      </message-driven>
      <message-driven>
         <ejb-name>QueueBean</ejb-name>
         <destination-jndi-name>queue/testQueue</destination-jndi-name>
         <ejb-ref>
            	<ejb-ref-name>ejb/StatelessRemote</ejb-ref-name>
            	<jndi-name>StatelessBean/remote</jndi-name>
         	</ejb-ref>
			<ejb-local-ref>
            	<ejb-ref-name>ejb/StatelessLocal</ejb-ref-name>
                <local-jndi-name>StatelessBean/local</local-jndi-name>
         	</ejb-local-ref>
            <resource-env-ref>           	
            	<resource-env-ref-name>res/aQueue</resource-env-ref-name>           
                <jndi-name>queue/testQueue</jndi-name>
         	</resource-env-ref>
      </message-driven>
      <message-driven>
         <ejb-name>TopicBean</ejb-name>
         <destination-jndi-name>topic/testTopic</destination-jndi-name>
      </message-driven>
      <message-driven>
         <ejb-name>DurableTopicBean</ejb-name>
         <destination-jndi-name>topic/testDurableTopic</destination-jndi-name>
         <mdb-user>dynsub</mdb-user>
         <mdb-passwd>dynsub</mdb-passwd>
         <mdb-subscription-id>DurableTopicTest</mdb-subscription-id>
      </message-driven>
      <message-driven>
         <ejb-name>ExQueueBean</ejb-name>
         <destination-jndi-name>queue/ex</destination-jndi-name>
      </message-driven>

      <message-driven>
         <ejb-name>QueueInADifferentContext</ejb-name>
         <destination-jndi-name>anotherContext/QueueInADifferentContext</destination-jndi-name>
      </message-driven>
      <message-driven>
         <ejb-name>TopicInADifferentContext</ejb-name>
         <destination-jndi-name>anotherContext/TopicInADifferentContext</destination-jndi-name>
      </message-driven>
   </enterprise-beans>
</jboss>
